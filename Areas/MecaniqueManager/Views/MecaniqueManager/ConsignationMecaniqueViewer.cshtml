@using DevExpress.AspNetCore
@using DevKbfSteel.Reports
@using DevKbfSteel.Helpers;
<link rel="stylesheet" href="~/css/thirdparty.bundle.css" />
<script src="~/js/thirdparty.bundle.js"></script>

<link rel="stylesheet" href="~/css/viewer.part.bundle.css" />
<script src="~/js/viewer.part.bundle.js"></script>
@{
    ViewData["Title"] = "Consignation N° " + XpertHelper.ConsignationMecanique;
}
<title>@ViewData["Title"]</title>
@{Consignation raport = new Consignation();
    raport.Parameters["NumConsignation"].Value = XpertHelper.ConsignationMecanique;
    raport.Name = "onsignation N°" + XpertHelper.NumDemandeFourniture;
    raport.RequestParameters = false;
    raport.Visible = true;
}
@Html.DevExpress().WebDocumentViewer("WebDocumentViewer").Height("1000px").Bind(raport);